{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "plugins": ["import"],
  "extends": ["airbnb", "eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "react/jsx-filename-extension": "Off",
    "react/jsx-one-expression-per-line": "off"
  }
}
